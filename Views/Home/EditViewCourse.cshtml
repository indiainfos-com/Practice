@model Registration_Form.Models.CourseModel
<table>
    <tr>
        
        @for (int i = 0; i < Model.Courses.Count; i++)
        {
          
            if (i % 3 == 0)
            {
            @:</tr><tr>
            }
            <td>

               
                 
                @Html.CheckBoxFor(m => m.Courses[i].IsCheck)
                <label>@Model.Courses[i].CourseName</label>
                
                @Html.HiddenFor(m => m.Courses[i].CourseId)
                @Html.HiddenFor(m => m.Courses[i].CourseName)

            </td>
        }
    </tr>
</table>

